{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.js"
        }
    ],
    "env": {
        "HUGGINGFACE_API_KEY": "@huggingface_api_key",
        "OFFICIAL_EMAIL": "@official_email"
    }
}